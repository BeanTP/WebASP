@{
    ViewBag.Title = "News";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}

<div class="blog-section">
    <div class="container">

        <div class="row">
            @if(ViewBag.meta == null)
            {
                Html.RenderAction("getAllNews", "News");
            } 
            else
            {
                <h3>@Model</h3>
            }
        </div>

    </div>
</div>
